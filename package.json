{
  "version": "1.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" --force"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "homepage": "https://github.com/stategen/toro-auto-router-plugin",
  "bugs": {
    "url": "https://github.com/stategen/toro-auto-router-plugin/issues",
    "email": "gotoanything@foxmail.com"
  },
  "keywords": [
    "taro",
    "router"
  ],
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "always",
    "trailingComma": "es5"
  },
  "name": "toro-auto-router-plugin",
  "author": "Thomas Trainset",
  "module": "dist/toro-auto-router-plugin.esm.js",
  "size-limit": [
    {
      "path": "dist/toro-auto-router-plugin.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/toro-auto-router-plugin.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "generated": "^1.4.0"
  },
  "dependencies": {
    "generated": "^1.4.0",
    "lodash.last": "^3.0.0",
    "ts-morph": "^9.1.0",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/lodash.last": "^3.0.6",
    "husky": "^4.3.6",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  }
}
